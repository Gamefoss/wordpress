append styles
  - wp_enqueue_style( 'socials-css', get_template_directory_uri() . "/library/css/modules/socials.css", false );
nav.socials
  - foreach ( get_field('socials', 'options') as $social ):
    - $social = (object) $social
    a.icon.social(class!=echo('"social-{$social->network}"') href!=echo('$social->url') target="_blank")
  - endforeach
